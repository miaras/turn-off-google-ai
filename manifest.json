{
  "manifest_version": 3,
  "name": "Hide Google AI Overview",
  "version": "1.1",
  "description": "Appends -ai to searches to hide AI content.",
  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/search*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}